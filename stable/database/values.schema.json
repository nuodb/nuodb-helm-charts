{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "required": [
        "database"
    ],
    "properties": {
        "database": {
            "type": "object",
            "required": [
                "archiveType"
            ],
            "properties": {
                "archiveType": {
                    "type": "string",
                    "pattern": "^(|lsa)$"
                }
            }
        }
    }
}